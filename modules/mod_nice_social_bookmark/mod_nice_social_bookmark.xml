	<params>
        <param name="isize" type="radio" label="Icon Size" default="32" description="Size of icons to be displayed">
		<option value="24">24px</option>
		<option value="32">32px</option>
		<option value="48">48px</option>
		<option value="64">64px</option>
   	</param>
	<param name="iset" type="list" default="aqu" label="Icon Set" description="Set of icons to be desplayed, u can check icons on website">
		<option value="aqu">Aquatic</option>
		<option value="ora">Orange round</option>
		<option value="log">Logorunner</option>
		<option value="rou">White round</option>
	</param>
	<param name="iposition" type="list" default="center" label="Icon position" description="Set positioin of icons to be desplayed">
		<option value="left">Left</option>
		<option value="center">Center</option>
		<option value="right">Right</option>
	</param>
	<param name="s1" type="radio" label="Facebook" default="yes" description="Display the icon of Facebook?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>
	<param name="s2" type="radio" label="MySpace" default="yes" description="Display the icon of MySpace?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>
	<param name="s3" type="radio" label="Twitter" default="yes" description="Display the icon of Twitter?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>
	<param name="s4" type="radio" label="Digg" default="yes" description="Display the icon of Digg?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>
	<param name="s5" type="radio" label="Delicious" default="yes" description="Display the icon of Delicious?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>
	<param name="s6" type="radio" label="Stumbleupon" default="yes" description="Display the icon of Stumbleupon?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>
	<param name="s7" type="radio" label="Google Bookmarks" default="yes" description="Display the icon of Google Bookmarks?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>

	<param name="s8" type="radio" label="RSS Feed" default="yes" description="Display the icon of RSS Feed?">
		<option value="yes">Yes</option>
		<option value="no">No</option>
   	</param>				
